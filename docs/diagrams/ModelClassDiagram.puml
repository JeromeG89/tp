@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyJobBook" as ReadOnlyJobBook
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class JobBook
Class ModelManager
Class UserPrefs

Class UniqueApplicationList
Class JobApplication {
    - companyName: String
    - role: String
    - status: Status
    - deadline: LocalDateTime
}
Class "<<enumeration>>\nStatus" as Status {
    APPLIED
    INPROGRESS
    REJECTED
}
Class Tag

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

JobBook .up.|> ReadOnlyJobBook

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyJobBook
ModelManager -left-> "1" JobBook
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

JobBook *--> "1" UniqueApplicationList
UniqueApplicationList --> "~* all" JobApplication
JobApplication *--> Status
JobApplication *--> "*" Tag

JobApplication -[hidden]up--> I
UniqueApplicationList -[hidden]right-> I

ModelManager --> "~* filtered" JobApplication
@enduml